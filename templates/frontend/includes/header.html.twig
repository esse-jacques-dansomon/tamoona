{#                            If user is connected#}
                            {% if  (app.user) %}
                            <li class="nav-item" ><a  class="nav-link"  target="_blank" href="{{ path('admin') }}" >Dashboard</a></li>
                            {% endif %}
                        </ul>
                    </div>
